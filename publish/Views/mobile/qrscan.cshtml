@{ Layout = null;}
<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="utf-8" />
    <meta name=apple-mobile-web-app-capable content="yes" />
    <meta name=viewport content="width=device-width,initial-scale=1,user-scalable=0" />
    <script type="text/javascript">
        //JS监听浏览器文字大小代码
        (function (doc, win) { var docEl = doc.documentElement, resizeEvt = 'orientationchange' in window ? 'orientationchange' : 'resize', recalc = () => { docEl.style.fontSize = (docEl.clientWidth / 7.5) + 'px' }; if (doc.addEventListener) { win.addEventListener(resizeEvt, recalc, false) } recalc() })(document, window);
    </script>
    <style>
        * {
            margin: 0;
            padding: 0;
            user-select: none;
        }

        #app {
            margin: auto;
            width: 7.5rem;
            display: inline-block;
            text-align:center;
        }
        .qr-msg {
            padding: 1rem;
            text-align: center;
        }
        .qr-icon {
            width: 3rem;
            height: 3rem;
            display: inline-block;
            vertical-align: middle;
            background: #303030;
            -webkit-mask: url(data:image/png;base64,) no-repeat 50% 50% /3rem;
        }
        .qr-text {
            color: #303030;
            font-size: 0.32rem;
            line-height: 1rem;
        }
        .qr-btns {
            width: 100%;
            bottom: 20%;
            text-align: center;
            position: absolute;
        }
        .qr-btn {
            width: 3rem;
            color: #1565C0;
            height: 0.7rem;
            font-size: 0.32rem;
            font-weight:500;
            display: inline-block;
            font-style: normal;
            text-indent: 0.1rem;
            line-height: 0.7rem;
            border-radius: 0.1rem;
            letter-spacing: 0.1rem;
            background-color: #9ecce0;
        }
    </style>
</head>
<body onselectstart="return false">
    <div id="app">
        <div class="qr-msg">
            <div class="qr-icon"></div>
            <div class="qr-text">登录管理后台</div>
        </div>
        <div class="qr-btns">
            <em class="qr-btn" v-on:click="doYes">登录</em>
        </div>
    </div>
    <script type="text/javascript" src="/js/vue.min.js"></script>
    <script type="text/javascript" src="@ViewBag.Assets/js/axios.min.js?@ViewBag.ResVersion"></script>
    <script src="https://g.alicdn.com/dingding/dingtalk-jsapi/2.7.13/dingtalk.open.js"></script>
    <script type="text/javascript">
        new Vue({
            el: '#app',
            methods: {
                doYes() {
                    try {
                        axios.get(location.pathname + '?do=confirm').then(res => { return res.data }).then(res => {
                            if (res.success) {
                                if (dd.ios || dd.android) {
                                    dd.device.notification.toast({ text: res.message })
                                    dd.biz.navigation.close()
                                }
                            } else {
                                if (dd.ios || dd.android) {
                                    dd.device.notification.toast({ text: res.message })
                                } else {
                                    alert(res.message)
                                }
                            }
                        });
                    } catch (e) { alert(JSON.stringify(e)) }
                }
            },
            mounted() {
                document.title = '扫码确认';
                try { dd.biz.navigation.setTitle({ title: '扫码确认' }); } catch (e) { }
            }
        })
    </script>
</body>
</html>